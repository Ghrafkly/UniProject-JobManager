<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<fx:root fx:id="findjobs" prefHeight="700.0" prefWidth="1300.0" type="AnchorPane" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <BorderPane prefHeight="700.0" prefWidth="1300.0" stylesheets="@../css/table.css">
         <center>
            <TableView fx:id="tbvJobs" prefHeight="700.0" prefWidth="1300.0" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn fx:id="tcTitle" prefWidth="75.0" text="Title" />
                  <TableColumn fx:id="tcCompany" prefWidth="75.0" text="Company" />
                  <TableColumn fx:id="tcSalary" prefWidth="75.0" text="Salary" />
                  <TableColumn fx:id="tcLocation" prefWidth="75.0" text="Location" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </center>
      </BorderPane>
   </children>
</fx:root>
